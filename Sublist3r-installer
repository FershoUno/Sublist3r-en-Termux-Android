# sublist3r Coded By Ahmed Aboul-Ela - @aboul3la
# GitHub : https://github.com/aboul3la/Sublist3r  

verde="\033[1;32m"

apt update -y
apt install git -y
mkdir -p tools
cd tools
git clone https://github.com/aboul3la/Sublist3r
cd Sublist3r
python2 setup.py install
pip2 install -r requirements.txt
ln -s sublist3r.py $PREFIX/bin/sublist3r
sleep 1 && echo -e  $verde"OK"

